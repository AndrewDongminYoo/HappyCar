{
  "name": "@radiuslab/wegooli-happy-car",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native rnx-start --reset-cache",
    "dev": "react-native rnx-start --host localhost --port 8080",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "(cd ios && bundle exec pod install)",
    "deps:check": "rnx-align-deps",
    "deps:fix": "rnx-align-deps --write"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "i18next": "^23.2.6",
    "react": "18.2.0",
    "react-i18next": "^13.0.1",
    "react-native": "^0.72.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-mmkv": "^2.10.1",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-vision-camera": "^2.15.4",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.0",
    "@react-navigation/devtools": "^6.0.19",
    "@rnx-kit/align-deps": "^2.2.4",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
    "@rnx-kit/cli": "^0.16.9",
    "@rnx-kit/metro-config": "^1.3.7",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.28",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.5",
    "prettier": "^2.8.8",
    "react-native-flipper": "^0.203.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": ">=3.3.1 <5.1.0"
  },
  "resolutions": {
    "semver": "^7.5.2"
  },
  "engines": {
    "node": ">=16"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "presets": [
        "microsoft/react-native"
      ],
      "requirements": [
        "react-native@0.72"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "gestures",
        "jest",
        "masked-view",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens"
      ]
    },
    "bundle": {
      "entryFile": "index.ts",
      "assetsDest": "dist",
      "plugins": [
        "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "ignoredModules": [
              "react-is"
            ]
          }
        ],
        "@rnx-kit/metro-plugin-typescript"
      ],
      "targets": [
        "ios",
        "android",
        "macos"
      ],
      "platforms": {
        "android": {
          "assetsDest": "dist/res"
        },
        "macos": {
          "plugins": [
            "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
            [
              "@rnx-kit/metro-plugin-duplicates-checker",
              {
                "ignoredModules": [
                  "react-is"
                ]
              }
            ]
          ]
        }
      }
    },
    "server": {
      "projectRoot": "src",
      "plugins": [
        "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "ignoredModules": [
              "react-is"
            ],
            "throwOnError": false
          }
        ],
        "@rnx-kit/metro-plugin-typescript"
      ]
    }
  }
}
